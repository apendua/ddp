{
  "extends": "airbnb",
  "rules": {
    "import/prefer-default-export": "off",
    "import/extensions": "off",
    "max-len": ["error", 140]
  },
  "parser": "babel-eslint",
  "parserOptions": {
    "ecmaVersion": 7,
    "sourceType": "module",
    "ecmaFeatures": {
      "experimentalObjectRestSpread": true
    }
  },
  "env": {
    "es6": true,
    "browser": true,
    "node": true
  },
  "plugins": [],
  "settings": {}
}
